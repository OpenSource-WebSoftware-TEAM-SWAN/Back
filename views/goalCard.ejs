<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/stylesheets/writeSeed.css">
    <link rel="stylesheet" href="/stylesheets/navibar.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">
    <!-- <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script> -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js" defer></script>
    <script defer src="/javascripts/mainJquery.js" defer></script>
    <script defer src="/javascripts/navibar.js" defer></script>
    <script defer src="/javascripts/writeSeed.js" defer></script>
    <link href="/stylesheets/bucket.css" rel="stylesheet">
    <link href="/stylesheets/seedShow.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
    <title>Document</title>
</head>

<body>
    <% feedArray.forEach(feed=>{ %>
        <div class="card">
            <img src="<%= feed.feedImgPath %>" class="card-img-top">
            <!-- feed Image -->
            <div class="card-body">
                <div class="card-text">
                    <h3>
                        <%= feed.feedGoal %>
                    </h3>
                    <p>
                        <%= feed.feedMemo %>
                    </p>
                    <p style="text-align:right;"> time </p>
                </div>
            </div>
        </div>
        <% }); %>

</body>
<script src="https://unpkg.com/masonry-layout@4/dist/masonry.pkgd.min.js" defer></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.imagesloaded/4.1.4/imagesloaded.pkgd.min.js" defer></script>

</html>